{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "types": [
            "cypress",
            "node"
        ],
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}